{
  "name": "micro-server",
  "version": "1.0.0",
  "description": "micro server for freelog",
  "private": true,
  "dependencies": {
    "egg": "^2.2.1",
    "egg-scripts": "^2.5.0",
    "extend": "^3.0.2",
    "http-proxy-middleware": "^0.19.0",
    "is-type-of": "^1.2.0",
    "koa-validate": "^1.0.7",
    "koa2-connect": "^1.0.2",
    "lodash": "^4.17.10",
    "object-path": "^0.11.4",
    "path-to-regexp": "^2.4.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^6.0.0",
    "husky": "^1.1.4",
    "webstorm-disable-index": "^1.2.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "node scripts/pre-push.js"
    }
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --title=egg-server-micro-server",
    "stop": "egg-scripts stop --title=egg-server-micro-server",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint . --fix",
    "ci": "npm run cov",
    "autod": "autod",
    "pack": "tar -zcf micro-server.tar app config app.js",
    "build:docker": "docker build -t=freelog/micro-server -f Dockerfile-aliyun ."
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "MIT"
}
